<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <!-- added for hologram purifier skin, the spine crawler uses its own model for its death, so we need to put in an extra check -->
    <CActorUnit id="SpineCrawler">
        <On Terms="UnitDeathCustomize.SpineCrawler; ModelName Hologram_Skin_SpineCrawler" Send="DeathCustomize DarkProtoss"/>
        <DeathCustoms ModelLink="Hologram_Death_Medium" Name="DarkProtoss"/>
    </CActorUnit>
</Catalog>