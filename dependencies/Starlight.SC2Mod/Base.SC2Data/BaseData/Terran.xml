    <CActorSimple default="1" id="TerranPlaceSimple">
        <?token id="Unit" type="CUnit"?>
        <On Terms="Effect.##Unit##Place.Create" Send="Create"/>
        <On Terms="Abil.##Unit##Reposition.SourceCastStart" Send="Create"/>
        <On Terms="Abil.##Unit##Reposition.SourcePrepStart" Send="Destroy"/>
        <On Terms="ActorCreation" Target="_Unit" Send="Create TerranPlacementSoundSmall"/>
        <On Terms="ActorCreation" Target="_Unit" Send="AnimClear Fidget 0.000000"/>
        <On Terms="ActorCreation" Target="_Unit" Send="AnimPlay Unload Unload,End 0 0.000000 0.000000"/>
        <On Terms="ActorCreation" Send="TimerSet 0.150000 BirthDust"/>
        <On Terms="ActorCreation" Send="TimerSet 0.250000 BirthSplat"/>
        <On Terms="TimerExpired; TimerName BirthDust" Send="Create MarinePlacementDust"/>
        <On Terms="TimerExpired; TimerName BirthSplat" Send="Create MarinePlacementSplat"/>
        <On Terms="AnimDone; AnimName Unload" Send="Destroy"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
    </CActorSimple>

