<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CEffectCreateHealer id="VoidSentryShieldRepair">
        <ValidatorArray value="noMarkers"/>
        <ValidatorArray value="ShieldsNotFull"/>
        <ValidatorArray value="NotWarpingIn"/>
        <ValidatorArray value="HiddenCompareAB"/>
        <ValidatorArray value="HiddenCompareBA"/>
        <ValidatorArray value="NotVortexd"/>
        <ValidatorArray value="NotUncommandable"/>
        <ValidatorArray value="SourceNotHidden"/>
        <ValidatorArray value="NotInterceptor"/>
        <ValidatorArray value="AbilityUsageRangeLimitRange5Unit"/>
        <ValidatorArray value="NotUnitSpawnerCaster"/>
        <EditorCategories value="Race:Terran"/>
        <PeriodicValidator value="ShieldsNotFull"/>
        <DrainVital value="Energy"/>
        <DrainVitalCostFactor value="0.33"/>
        <RechargeVital value="Shields"/>
        <RechargeVitalRate value="12"/>
    </CEffectCreateHealer>
    <CActorBeamSimple id="SentryShieldRepairBeam" parent="BeamSimpleAnimationStyleContinuous">
        <Model value="Sentry_HealingBeam"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStart" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStop" Send="AnimBracketStop BSD"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStart" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStop" Send="AnimBracketStop BSD"/>
        <HostForProps Subject="_Unit" Scope="Caster" Actor="Find"/>
        <HostLaunch Subject="_Unit" Scope="Caster" Actor="Find"/>
        <HostImpact Subject="_Unit" Scope="Target" Actor="Find"/>
        <HostLaunchSiteOps Ops="SOpAttachWeapon"/>
        <HostImpactSiteOps Ops="SOpAttachCenter"/>
        <Inherits index="Visibility" value="1"/>
        <ModelFlags index="AutomateGlobalLoops" value="1"/>
    </CActorBeamSimple>
    <CActorModel id="SentryShieldRepairModel" parent="ModelAnimationStyleContinuous">
        <Model value="Sentry_HealingBeam_Impact"/>
        <On Terms="ActorCreation" Send="$Birth PlayForever"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStart; At Target" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStop; At Target" Send="AnimBracketStop BSD"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStart; At Target" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStop; At Target" Send="AnimBracketStop BSD"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachCenter"/>
        <ModelFlags index="AutomateGlobalLoops" value="1"/>
    </CActorModel>
    <CActorSound id="SentryShieldRepairSoundLoop" parent="SoundContinuous">
        <Sound value="SentryShieldRepair_Loop"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStart; At Target" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepair.TargetStop; At Target" Send="Destroy"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStart; At Target" Send="Create"/>
        <On Terms="Effect.VoidSentryShieldRepairDouble.TargetStop; At Target" Send="Destroy"/>
        <On Terms="Effect.SentryRepairTargetPersistent.Start; At Target" Send="Create"/>
        <On Terms="Effect.SentryRepairTargetPersistent.Stop; At Target" Send="Destroy"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <On Terms="ActorCreation" Send="Create SentryShieldRepairSoundStart"/>
        <On Terms="ActorDestruction" Send="Create SentryShieldRepairSoundStop"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachOverhead"/>
    </CActorSound>
    <CModel id="Sentry_HealingBeam" parent="PersistentSpellFX">
        <Model value="Assets\Effects\Protoss\Sentry_HealingBeam\Sentry_HealingBeam.m3"/>
        <EditorCategories value="Race:Protoss"/>
    </CModel>
    <CModel id="Sentry_HealingBeam_Impact" parent="PersistentSpellFX">
        <Model value="Assets\Effects\Protoss\Sentry_HealingBeam_Impact\Sentry_HealingBeam_Impact.m3"/>
        <EditorCategories value="Race:Protoss"/>
    </CModel>
    <CSound id="SentryShieldRepair_Loop" parent="Spell">
        <EditorCategories value="Race:Protoss"/>
        <AssetArray File="Assets\Sounds\ShieldBattery_ReCharge2_Loop.wav" FacialGroup=""/>
        <LoopCount value="-1"/>
        <Pitch value="1.000000,2.000000"/>
        <Volume value="-11.000000,-11.000000"/>
    </CSound>
</Catalog>
