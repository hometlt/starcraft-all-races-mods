<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CDataCollectionPattern default="1" id="LotC_UnitBase">
        <Fields Reference="Unit,^ParamId^,Name"/>
        <Fields Reference="Unit,^ParamId^,Description"/>
        <Fields Reference="Unit,^ParamId^,Subtitle"/>
        <Fields Reference="Actor,^ParamId^,UnitKillRank"/>
        <Fields Reference="Model,^ParamId^,Model"/>
        <Fields Reference="Model,^ParamId^Portrait,Model" NameOverride="DataCollectionPattern/LotC_Building/PortraitModel"/>
        <Fields Reference="Unit,^ParamId^,LifeStart"/>
        <Fields Reference="Unit,^ParamId^,LifeMax"/>
        <Fields Reference="Unit,^ParamId^,LifeArmor"/>
        <Fields Reference="Unit,^ParamId^,Sight"/>
        <Fields Reference="Unit,^ParamId^,Food"/>
        <Fields Reference="Unit,^ParamId^,CostResource"/>
        <Fields Reference="Unit,^ParamId^,CardLayouts"/>
        <Fields Reference="Unit,^ParamId^,EnergyStart"/>
        <Fields Reference="Unit,^ParamId^,EnergyMax"/>
        <Fields Reference="Unit,^ParamId^,EnergyRegenRate"/>
        <Fields Reference="Unit,^ParamId^,Speed"/>
        <Fields Reference="Unit,^ParamId^,AbilArray"/>
        <Fields Reference="Unit,^ParamId^,Attributes"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="LotC_AbilTrain">
        <Fields Reference="Abil,^ParamId^,InfoArray"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="LotC_Unit" parent="LotC_UnitBase">
        <Fields index="0" Reference="DataCollection,^ParamId^,Name" UserData="test"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="LotC_Building" parent="LotC_UnitBase"/>
    <CDataCollectionPattern id="LotC_WeaponInstant">
        <Fields Reference="Effect,^ParamId^Damage,Amount"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="LotC_Upgrade">
        <Fields Reference="Button,^ParamId^,Icon"/>
        <Fields Reference="Upgrade,^ParamId^,EffectArray"/>
    </CDataCollectionPattern>
</Catalog>
