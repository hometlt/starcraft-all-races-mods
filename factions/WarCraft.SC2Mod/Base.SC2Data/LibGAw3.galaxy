    int lv_heroCount;
    fixed lv_sharedXP;
    fixed lv_currentXP;
    int lv_level;
    fixed lv_faction;
    int lv_gold;

    // Automatic Variable Declarations
    unitgroup autoD5038019_g;
    int autoD5038019_u;

    // Variable Initialization
    lv_unit = EventUnit();
    lv_uT = UnitGetType(lv_unit);
    lv_killer = EventUnitDamageSourceUnit();
    lv_killerPlayer = UnitGetOwner(lv_killer);
    lv_position = UnitGetPosition(lv_unit);
    lv_xP = UnitGetPropertyFixed(lv_unit, c_unitPropKillXP, c_unitPropCurrent);
    lv_heroGroup = UnitGroupSearch(null, lv_killerPlayer, lv_position, libGAw3_gv_xPShareRadius, UnitFilter((1 << c_targetFilterHeroic), 0, (1 << c_targetFilterAlly) | (1 << c_targetFilterNeutral) | (1 << c_targetFilterEnemy) | (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32)) | (1 << (c_targetFilterHallucination - 32))), 0);
    lv_gold = CatalogFieldValueGetAsInt(c_gameCatalogUnit, lv_uT, "KillResource[Terrazine]", libGAw3_gv_creepPlayer);

    // Conditions
    if (testConds) {
        if (!((lv_killerPlayer != 16))) {
            return false;
        }

        if (!((PlayerGetAlliance(lv_killerPlayer, c_allianceIdPassive, libGAw3_gv_creepPlayer) == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    TextExpressionSetToken("Param/Expression/lib_GAw3_8A98053C", "Amount", IntToText(lv_gold));
    libGAw3_gf_CreateWC3StyleTextTag(TextExpressionAssemble("Param/Expression/lib_GAw3_8A98053C"), libGAw3_gv_goldTextColor, PlayerGroupSingle(lv_killerPlayer), lv_position, libGAw3_gv_resourceTextVelocity, libGAw3_gv_resourceTextLifetime, libGAw3_gv_resourceTextLifetime);
    if ((UnitHasBehavior(lv_killer, "SuppressXPGrantOnKill") == false)) {
        if ((libNtve_gf_UnitGroupIsDead(lv_heroGroup) == true)) {
            lv_heroGroup = UnitGroupSearch(null, lv_killerPlayer, lv_position, 500.0, UnitFilter((1 << c_targetFilterHeroic), 0, (1 << c_targetFilterAlly) | (1 << c_targetFilterNeutral) | (1 << c_targetFilterEnemy) | (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32)) | (1 << (c_targetFilterHallucination - 32))), 0);
        }

        lv_heroCount = UnitGroupCount(lv_heroGroup, c_unitCountAlive);
        if ((libNtve_gf_UnitGroupIsDead(lv_heroGroup) == false)) {
            lv_sharedXP = (lv_xP / lv_heroCount);
            autoD5038019_g = lv_heroGroup;
            autoD5038019_u = UnitGroupCount(autoD5038019_g, c_unitCountAll);
            for (;; autoD5038019_u -= 1) {
                lv_hero = UnitGroupUnitFromEnd(autoD5038019_g, autoD5038019_u);
                if (lv_hero == null) { break; }
                lv_level = UnitLevel(lv_hero);
                lv_faction = (UserDataGetFixed("GameBalanceArrayValues", "Hero XP Gained - Creep Reduction Table", "Values", lv_level) / 100.0);
                if ((lv_faction != 0.0)) {
                    lv_currentXP = UnitGetPropertyFixed(lv_hero, c_unitPropXP, c_unitPropCurrent);
                    lv_currentXP += Ceiling((lv_faction * lv_sharedXP));
                    UnitSetPropertyFixed(lv_hero, c_unitPropXP, lv_currentXP);
                }

            }
        }

    }

    return true;
}

