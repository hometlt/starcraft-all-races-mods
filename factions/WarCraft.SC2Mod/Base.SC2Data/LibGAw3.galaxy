    auto6F174D30_ae = (lv_c - 1);
    lv_i = 0;
    for ( ; ( (auto6F174D30_ai >= 0 && lv_i <= auto6F174D30_ae) || (auto6F174D30_ai < 0 && lv_i >= auto6F174D30_ae) ) ; lv_i += auto6F174D30_ai ) {
