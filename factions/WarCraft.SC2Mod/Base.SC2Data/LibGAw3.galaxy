void GAw3_SetUnitBehaviorCount(unit u,string behavior,int amount){
    int count=(amount-UnitBehaviorCount(u,behavior));
    if (count==0){return;}
    if (count>0){UnitBehaviorAdd(u,behavior,u,count);}
    else{UnitBehaviorRemove(u,behavior,count*-1);}
}
