    lv_dropOrder = OrderTargetingUnit(AbilityCommand("TodGodInventory", 0), lp_u);

    // Implementation
    OrderSetTargetItem(lv_dropOrder, lp_item);
    UnitIssueOrder(libGAw3_gv_god, OrderTargetingUnit(AbilityCommand("TodGodInventory", 2), lp_item), c_orderQueueReplace);
    if ((UnitOrderIsValid(libGAw3_gv_god, lv_dropOrder) == true)) {
        UnitIssueOrder(libGAw3_gv_god, lv_dropOrder, c_orderQueueAddToEnd);
