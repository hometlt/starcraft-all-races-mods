<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <Frame type="UnitStatusFrame" name="RaidersRaynor_StatusFrame" template="Coop_UnitStatus_Tychus/TychusSquadUnitStatusFrameTemplate">
        <Frame type="UnitStatusUnitName" name="UnitNameLabel">
            
            <Frame type="Label" name="NicknameLabel">  
                <Text val="@Unit/Name/RaidersRaynorNickname"/>
            </Frame>
        </Frame>   
    </Frame>
    
    <Frame type="UnitStatusFrame" name="RaidersSwann_StatusFrame" template="Coop_UnitStatus_Tychus/TychusSquadUnitStatusFrameTemplate">
        
        <Frame type="UnitStatusUnitName" name="UnitNameLabel">
            
            <Frame type="Label" name="NicknameLabel">  
                <Text val="@Unit/Name/RaidersSwannNickname"/>
            </Frame>
        </Frame>   
    </Frame>

    <Frame type="UnitStatusFrame" name="RaidersTosh_StatusFrame" template="Coop_UnitStatus/HeroUnitEnergyStatusFrameTemplate">
        <Height val="55"/>

        <Frame type="UnitStatusShieldsBar" name="ShieldsBar" template="Coop_UnitStatus/NovaStatusShieldsBarTemplate">
            <Height val="8"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="32"/>
        </Frame>

        <Frame type="UnitStatusEnergyBar" name="EnergyBar" template="Coop_UnitStatus/UnitStatusSegmentedEnergyTemplate">
            <Height val="8"/>
        </Frame>

        <Frame type="UnitStatusHealthBar" name="HealthBar" template="Coop_UnitStatus/UnitStatusHealthBarTemplate">
            <Segmented val="true"/>
            <SegmentValue val="50"/>
        </Frame>

        <Frame type="UnitStatusUnitName" name="UnitNameLabel" template="Coop_UnitStatus/UnitStatusUnitNameTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Label" name="Label">
                <Style val="HotS_KerriganUnitStatusNameLabel"/>
            </Frame>
        </Frame>
        <Frame type="Image" name="NovaBackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
            <Anchor side="Bottom" relative="$parent/EnergyBar" pos="Max" offset="16"/>
            <Anchor side="Left" relative="$parent/EnergyBar" pos="Min" offset="-10"/>
            <Anchor side="Right" relative="$parent/EnergyBar" pos="Max" offset="9"/>
            <Texture val="@@UI/HotS_UnitStatus_NovaHealthFrame"/>
            <RenderPriority val="500"/>
            <RenderType val="HDR"/>
        </Frame>
    </Frame>

    <Frame type="UnitStatusFrame" name="RaidersStetmann_StatusFrame" template="Coop_UnitStatus/HeroUnitEnergyStatusFrameTemplate">
        <Height val="55"/>

        <Frame type="UnitStatusShieldsBar" name="ShieldsBar" template="Coop_UnitStatus/NovaStatusShieldsBarTemplate">
            <Height val="8"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="32"/>
        </Frame>

        <Frame type="UnitStatusEnergyBar" name="EnergyBar" template="Coop_UnitStatus/UnitStatusSegmentedEnergyTemplate">
            <Height val="8"/>
        </Frame>

        <Frame type="UnitStatusHealthBar" name="HealthBar" template="Coop_UnitStatus/UnitStatusHealthBarTemplate">
            <Segmented val="true"/>
            <SegmentValue val="50"/>
        </Frame>

        <Frame type="UnitStatusUnitName" name="UnitNameLabel" template="Coop_UnitStatus/UnitStatusUnitNameTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Label" name="Label">
                <Style val="HotS_KerriganUnitStatusNameLabel"/>
            </Frame>
        </Frame>
        <Frame type="Image" name="NovaBackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
            <Anchor side="Bottom" relative="$parent/EnergyBar" pos="Max" offset="16"/>
            <Anchor side="Left" relative="$parent/EnergyBar" pos="Min" offset="-10"/>
            <Anchor side="Right" relative="$parent/EnergyBar" pos="Max" offset="9"/>
            <Texture val="@@UI/HotS_UnitStatus_NovaHealthFrame"/>
            <RenderPriority val="500"/>
            <RenderType val="HDR"/>
        </Frame>
    </Frame>

</Desc>
