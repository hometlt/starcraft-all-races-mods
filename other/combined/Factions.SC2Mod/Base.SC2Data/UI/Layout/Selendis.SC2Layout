        </Frame>

        <Frame type="CommandButton" name="CommandButton10" template="CommandPanel/CommandCardButtonTemplate">
            <Visible val="false"/>
        </Frame>

        <Frame type="CommandButton" name="CommandButton11" template="CommandPanel/CommandCardButtonTemplate">
