            <Anchor side="Bottom" relative="$parent/CommandButton05" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/CommandButton05" pos="Max" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
