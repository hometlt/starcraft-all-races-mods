            <Texture val="Assets\Textures\btn-ability-terran-calldownextrasupplies.dds"/>
        </Frame>

        <Frame type="Frame" name="TechPurchaseFanfare">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
    
            <Width val="285"/>
            <Height val="285"/>
            <Visible val="False"/>
            <Unclipped val="True"/>
    
            <Animation name="FanfarePlay">
                <Event event="PurchaseStart" action="Reset,Play" frame="$parent"/>
                <Event event="OnShown" action="Reset,Play" frame="$this"/>
                <Event event="OnHidden" action="Reset,Pause" frame="$this"/>
    
                <Controller type="Sound" end="Pause" frame="$this">
                    <Key type="Sound" time="0.0" sound="@TUI_ArmoryConsolePurchase"/>
                </Controller>
    
                <!-- This sound is for the credits countdown
                    
                <Controller type="Sound" end="Pause" frame="$this">
                    <Key type="Sound" time="0.0" sound="@UI_ScoreScreenMissionCreditsLoop"/>
                </Controller-->
    
                <Controller type="Visibility" end="Pause" frame="$this/Scanlines">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
    
                <Controller type="Visibility" end="Pause" frame="$this/RightFlash">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
    
                <Controller type="Visibility" end="Pause" frame="$this/BottomFlash">
                    <Key type="Bool" time="0.05" value="True"/>
                </Controller>
    
                <Controller type="Visibility" end="Pause" frame="$this/LeftFlash">
                    <Key type="Bool" time="0.1" value="True"/>
                </Controller>
    
                <Controller type="Visibility" end="Pause" frame="$this/TopFlash">
                    <Key type="Bool" time="0.15" value="True"/>
                </Controller>
    
                <Controller type="Event" end="Pause" frame="$this/Scanlines">
                    <Key type="Event" time="0.95" event="StopScanlines"/>
                </Controller>
    
                <Controller type="Visibility" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="True"/>
                    <Key type="Bool" time="1.1" value="False"/>
                </Controller>
            </Animation>
    
            <Frame type="Image" name="Scanlines">
                <Anchor side="Top" relative="$parent/CenterAnchor" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/CenterAnchor" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/CenterAnchor" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/CenterAnchor" pos="Max" offset="0"/>
                <Visible val="False"/>
    
                <LayerCount val="2"/>
                <Alpha val="200"/>
    
                <Color val="FFFFFF" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                
                <Texture val="Assets\Textures\ui_techpurchase_upgradefanfare_upgradefanfare_scanlines.dds" layer="1"/>
                <TextureCoords top="0" left="1" bottom="1" right="0" layer="1"/>
                <BlendMode val="Add" layer="1"/>
                <WrapUV val="True" layer="1"/>
    
                <Animation name="ShimmerAnim">
                    <Event event="OnShown" action="DirectionForward" frame="$this"/>
                    <Event event="OnShown" action="Play" frame="$this"/>
                    <Event event="OnHidden" action="Reset,Stop" frame="$this"/>
    
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.1" value="200"/>
                    </Controller>
                
                    <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="-1" out="linear"/>
                        <Key type="Curve" time="0.8" value="1" in="linear"/>
                    </Controller>
    
                    <Controller type="LayerUV" layer="1" side="Bottom" relative="False" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="-1" out="linear"/>
                        <Key type="Curve" time="0.8" value="1" in="linear"/>
                    </Controller>
                </Animation>
    
                <Animation name="ScanlineStop">
                    <Event event="StopScanlines" action="Play" frame="$this"/>
    
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="200"/>
                        <Key type="Curve" time="0.15" value="0"/>
                    </Controller>
    
                    <Controller type="Visibility" end="Pause" frame="$this">
                        <Key type="Bool" time="0.15" value="False"/>
                    </Controller>
                </Animation>
            </Frame>
    
            <Frame type="Frame" name="CenterAnchor">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                
                <Width val="65"/>
                <Height val="65"/>
            </Frame>
    
            <Frame type="Image" name="RightFlash">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/CenterAnchor" pos="Max" offset="0"/>
                <Width val="40"/>
                <Height val="70"/>
                <Rotation val="180"/>
                <Texture val="Assets/Textures/ui_techpurchase_upgradefanfare_left.dds"/>
                <Visible val="False"/>
                <Alpha val="200"/>
    
                <Animation name="ShineGrow">
                    <Event event="OnShown" action="Reset,Play" frame="$this"/>
    
                    <Controller type="Dimension" end="Pause" frame="$this" dimension="Width" relative="False">
                        <Key type="Curve" time="0.0" value="20"/>
                        <Key type="Curve" time="0.2" value="100"/>
                        <Key type="Curve" time="0.35" value="25"/>
                        <Key type="Curve" time="0.45" value="40"/>
                    </Controller>
            
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.2" value="255"/>
                        <Key type="Curve" time="0.35" value="200"/>
                        <Key type="Curve" time="0.45" value="100"/>
                        <Key type="Curve" time="0.5" value="0"/>
                    </Controller>
                </Animation>
            </Frame>
    
            <Frame type="Image" name="LeftFlash">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/CenterAnchor" pos="Min" offset="0"/>
                <Width val="40"/>
                <Height val="70"/>
                <Texture val="Assets/Textures/ui_techpurchase_upgradefanfare_left.dds"/>
                <Visible val="False"/>
    
                <Animation name="ShineGrow">
                    <Event event="OnShown" action="Reset,Play" frame="$this"/>
    
                    <Controller type="Dimension" end="Pause" frame="$this" dimension="Width" relative="False">
                        <Key type="Curve" time="0.0" value="20"/>
                        <Key type="Curve" time="0.2" value="100"/>
                        <Key type="Curve" time="0.35" value="25"/>
                        <Key type="Curve" time="0.45" value="40"/>
                    </Controller>
            
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.2" value="255"/>
                        <Key type="Curve" time="0.35" value="200"/>
                        <Key type="Curve" time="0.45" value="100"/>
                        <Key type="Curve" time="0.5" value="0"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="TopFlash">
                <Anchor side="Bottom" relative="$parent/CenterAnchor" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="70"/>
                <Height val="40"/>
                <Texture val="Assets/Textures/ui_techpurchase_upgradefanfare_top.dds"/>
                <Visible val="False"/>
    
                <Animation name="ShineGrow">
                    <Event event="OnShown" action="Reset,Play" frame="$this"/>
    
                    <Controller type="Dimension" end="Pause" frame="$this" dimension="Height" relative="False">
                        <Key type="Curve" time="0.0" value="20"/>
                        <Key type="Curve" time="0.2" value="100"/>
                        <Key type="Curve" time="0.35" value="25"/>
                        <Key type="Curve" time="0.45" value="40"/>
                    </Controller>
            
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.2" value="255"/>
                        <Key type="Curve" time="0.35" value="200"/>
                        <Key type="Curve" time="0.45" value="100"/>
                        <Key type="Curve" time="0.5" value="0"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="BottomFlash">
                <Anchor side="Top" relative="$parent/CenterAnchor" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Rotation val="180"/>
                <Width val="70"/>
                <Height val="40"/>
                <Texture val="Assets/Textures/ui_techpurchase_upgradefanfare_top.dds"/>
                <Visible val="False"/>
    
                <Animation name="ShineGrow">
                    <Event event="OnShown" action="Reset,Play" frame="$this"/>
        
                    <Controller type="Dimension" end="Pause" frame="$this" dimension="Height" relative="False">
                        <Key type="Curve" time="0.0" value="20"/>
                        <Key type="Curve" time="0.2" value="100"/>
                        <Key type="Curve" time="0.35" value="25"/>
                        <Key type="Curve" time="0.45" value="40"/>
                    </Controller>
            
                    <Controller type="Fade" end="Pause" frame="$this">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.2" value="255"/>
                        <Key type="Curve" time="0.35" value="200"/>
                        <Key type="Curve" time="0.45" value="100"/>
                        <Key type="Curve" time="0.5" value="0"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor relative="$parent"/>
        </Frame>

        <StateGroup name="ToggleState">

            <State name="Normal">
                <When type="Property" frame="$this" Toggled="false"/>
            </State>

            <State name="Over">
                <When type="Property" frame="$this" Toggled="true"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="0,255,0"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="0,255,0"/>
            </State>
        </StateGroup>

        <StateGroup name="TechPurchaseState">
            <DefaultState val="Available"/>
            
            <State name="Available">
                <When type="AnimationState" frame="$this" TechPurchaseState="Available"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="429936"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="429936"/>
                <Action type="SetProperty" frame="$this/Icon" AdjustmentColor="429936"/>
            </State>
			
            <State name="Purchased">
                <When type="AnimationState" frame="$this" TechPurchaseState="Purchased"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="314736"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="314736"/>
				<Action type="SetProperty" frame="$this/Icon" AdjustmentColor="314736"/>
            </State>
			
            <State name="NoCredit">
                <When type="AnimationState" frame="$this" TechPurchaseState="NoCredit"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="170,49,49"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="170,49,49"/>
				<Action type="SetProperty" frame="$this/Icon" AdjustmentColor="170,49,49"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Frame" name="TechUnitFrameTemplate">
        <Anchor side="Left" relative="$sibling-1" pos="Max" offset="-20"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-24"/>

        <Width val="230"/>
        <Height val="360"/>
        <Visible val="False"/>
        <CollapseLayout val="True"/>

        <Frame type="Image" name="GridBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="22"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="23"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-23"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-88"/>

            <Texture val="Assets\Textures\ui_nova_storymode_bggrid.dds"/>
            <Tiled val="True"/>
            <Color val="429936"/>
            <Alpha val="80"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundImage">
            <Anchor relative="$parent"/>

            <Texture val="Assets/Textures/ui_techpurchase_unitbutton_availableselected.dds"/>
            <StateCount val="2"/>
            <StateIndex val="0"/>
            <DesaturateOnDisable val="False"/>
            <DisabledAlpha val="128"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="429936"/>
        </Frame>

        <Frame type="Label" name="UnitLabel">
			<Anchor side="Top" relative="$parent" pos="Min" offset="31"/>
			<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>

			<Style val="KL_Armory_UnitNameLabel_Available"/>
            <Text val="BASE"/>
		</Frame>

        <Frame type="CutsceneFrame" name="UnitModel">
			<Anchor side="Top" relative="$parent" pos="Min" offset="60"/>
			<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
			<Height val="210"/>
			<Width val="210"/>

			<File val="Cutscenes/UI_TechUnits_Loomings.SC2Cutscene"/>
            <RenderType val="HDR"/>
            <AutoPlay val="True"/>

            <Animation name="Birth">
                <Event event="OnShown" action="Reset,Play" frame="$parent/$parent"/>
                <Controller type="Cutscene" end="Stop">
                   <Key type="Cutscene" time="0" action="Bookmark" bookmark="SectionCCStart"/>
                   <Key type="Cutscene" time="0" action="Play"/>
                </Controller>
             </Animation>
		</Frame>

        <Frame type="Button" name="TechButton1" template="KLClassic_ReadyRoomTerran/TechButtonTemplate">
			<Anchor side="Right" relative="$parent" pos="Mid" offset="5"/>
        </Frame>

        <StateGroup name="SingleAbility">
            <State name="Normal">

            </State>

            <State name="Single">
                <When type="Property" frame="$this/TechButton2" Visible="False"/>

                <Action type="SetAnchor" frame="$this/TechButton1" side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Action type="SetAnchor" frame="$this/TechButton1" side="Right" relative="$parent" pos="Mid" offset="0"/>
            </State>
        </StateGroup>

        <Frame type="Button" name="TechButton2" template="KLClassic_ReadyRoomTerran/TechButtonTemplate">
			<Anchor side="Left" relative="$parent" pos="Mid" offset="-5"/>
        </Frame>

        <StateGroup name="SelectionState">
            
            <State name="Normal">
                <When type="Property" frame="$this/TechButton1" Toggled="false"/>
                <When type="Property" frame="$this/TechButton2" Toggled="false"/>

                <Action type="SetProperty" frame="$this/BackgroundImage" StateIndex="0"/>
            </State>
			
            <State name="Tech1InUse">
                <When type="Property" frame="$this/TechButton1" Toggled="true"/>
                <When type="Property" frame="$this/TechButton2" Toggled="false"/>

                <Action type="SetProperty" frame="$this/BackgroundImage" StateIndex="1"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="0,255,0"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitSelected"/>
            </State>
			
            <State name="Tech2InUse">
                <When type="Property" frame="$this/TechButton1" Toggled="false"/>
                <When type="Property" frame="$this/TechButton2" Toggled="true"/>

                <Action type="SetProperty" frame="$this/BackgroundImage" StateIndex="1"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="0,255,0"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitSelected"/>
            </State>
        </StateGroup>

        <StateGroup name="PurchaseState">
            <DefaultState val="Available"/>
            
            <State name="Available">
                <When type="AnimationState" PurchaseState="Available"/>

				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="0,255,0"/>
            </State>
			
            <State name="Purchased">
                <When type="AnimationState" PurchaseState="Purchased"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="429936"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="KL_Armory_UnitNameLabel_Purchased"/>
            </State>
			
            <State name="NoCredit">
                <When type="AnimationState" PurchaseState="NoCredit"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="170,49,49"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="KL_Armory_UnitNameLabel_NoCredit"/>
            </State>
			
            <State name="NewTech">
                <When type="AnimationState" PurchaseState="NewTech"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" Texture="Assets\Textures\ui_techpurchase_unitbutton_normaloverpressedover.dds"/>
				<Action type="SetProperty" frame="$this/BackgroundImage" AdjustmentColor="0,255,0"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="KL_Armory_UnitNameLabel_NewTech"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Frame" name="TechCategoryContainerTemplate">
        <Anchor relative="$parent"/>
        <Visible val="False"/>

        <Frame type="Frame" name="TechUnitFrame1" template="KLClassic_ReadyRoomTerran/TechUnitFrameTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
        </Frame>

        <Frame type="Frame" name="TechUnitFrame2" template="KLClassic_ReadyRoomTerran/TechUnitFrameTemplate">
        </Frame>

        <Frame type="Frame" name="TechUnitFrame3" template="KLClassic_ReadyRoomTerran/TechUnitFrameTemplate">
        </Frame>

        <Frame type="Frame" name="TechUnitFrame4" template="KLClassic_ReadyRoomTerran/TechUnitFrameTemplate">
        </Frame>

        <Frame type="Frame" name="TechUnitFrame5" template="KLClassic_ReadyRoomTerran/TechUnitFrameTemplate">
        </Frame>

        <Frame type="TabControl" name="TechCategoryTabControl">
            <SelectedIndex val="15"/>

            <TabListItem val="$parent/TechUnitFrame1/TechButton1"/>
            <TabListItem val="$parent/TechUnitFrame1/TechButton2"/>
            <TabListItem val="$parent/TechUnitFrame2/TechButton1"/>
            <TabListItem val="$parent/TechUnitFrame2/TechButton2"/>
            <TabListItem val="$parent/TechUnitFrame3/TechButton1"/>
            <TabListItem val="$parent/TechUnitFrame3/TechButton2"/>
            <TabListItem val="$parent/TechUnitFrame4/TechButton1"/>
            <TabListItem val="$parent/TechUnitFrame4/TechButton2"/>
            <TabListItem val="$parent/TechUnitFrame5/TechButton1"/>
            <TabListItem val="$parent/TechUnitFrame5/TechButton2"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="ArmoryPanelTemplate">
        <Anchor relative="$parent"/>

