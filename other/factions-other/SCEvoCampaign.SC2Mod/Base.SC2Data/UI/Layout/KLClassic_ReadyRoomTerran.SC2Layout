<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Frame type="Button" name="ArmoryItemButtonTemplate">
        <Anchor side="Top" relative="$sibling-1" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>

        <Visible val="True"/>
        <Height val="90"/>
        <CollapseLayout val="True"/>
        <Visible val="False"/>

        <NormalImage val="NormalImage"/>
        <HoverImage val="HoverImage"/>
        <HitTestFrame val="HitTestFrame"/>
        <Toggleable val="True"/>
        <ClickSound val="@UED_UI_Click"/>
        <AcceptsMouse val="True"/>
        <Style val="KL_ArmoryItemCategoryLabel"/>

        <StateGroup name="ToggledState">
            <State name="Normal">
                <When type="Property" frame="$this" Toggled="False"/>

                <Action type="SetProperty" frame="$this" AcceptsMouse="True"/>
                <Action type="SetProperty" frame="$this/NormalImage" Texture="Assets\Textures\ui_techpurchase_tabbutton_normalpressed.dds"/>
            </State>

            <State name="Toggled">
                <When type="Property" frame="$this" Toggled="True"/>

                <Action type="SetProperty" frame="$this" AcceptsMouse="False"/>
                <Action type="SetProperty" frame="$this/NormalImage" Texture="Assets\Textures\ui_techpurchase_tabbutton_normaloverpressedover.dds"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>

            <TextureType val="Border"/>
            <StateCount val="2"/>
            <Texture val="Assets\Textures\ui_techpurchase_tabbutton_normalpressed.dds"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="2fb32c"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>

            <TextureType val="Border"/>
            <StateCount val="2"/>
            <Texture val="Assets\Textures\ui_techpurchase_tabbutton_normaloverpressedover.dds"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="04ff00"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="EffectsItemTemplate">
        <Anchor side="Top" relative="$sibling-1" pos="Max" offset="3"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
        <Height val="20"/>

        <PreserveAnchorOffset val="True"/>
        <AnchorUseDeviceOffset val="True"/>
        <CollapseLayout val="True"/>
        <Visible val="False"/>

        <Frame type="Image" name="BulletPoint">
