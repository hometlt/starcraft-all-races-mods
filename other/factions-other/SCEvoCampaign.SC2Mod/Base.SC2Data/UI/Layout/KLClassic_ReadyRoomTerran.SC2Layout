            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>

            <Texture val="@UI/Glues/Pageassets_BulletPoint_Terr"/>
            <Width val="13"/>
            <Height val="20"/>
        </Frame>

        <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BulletPoint" pos="Max" offset="4"/>
            
            <Style val="KL_ArmoryEffectPositiveLabel"/>

            <StateGroup name="EmptyText">
                <State name="HasNoText">
                    <When type="Property" frame="$this" operator="NotEqual" Text=""/>

                    <Action type="SetProperty" frame="$parent" Visible="True"/>
                </State>

                <State name="Default">
                    <Action type="SetProperty" frame="$parent" Visible="False"/>
