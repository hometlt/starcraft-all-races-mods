<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Constant name="KL_DarkTerran" val="071304"/>
    <Constant name="KL_DarkProtoss" val="040b15"/>
    <Constant name="KL_DarkZerg" val="140901"/>

    <Constant name="FillColor_Terr" val="327e1a"/>
    <Constant name="FillColor_Prot" val="00f7f7"/>
