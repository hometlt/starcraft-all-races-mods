    <Constant name="ButtonFillNormal_Prot" val="001a1a"/>

    <Frame type="StandardTooltip" name="ClassicTerranTooltipTemplate" template="VoidShared_ScoreScreen/ScoreScreenTooltipTemplate">
        <PositionFixed val="false"/>

        <StateGroup name="TextNullCheck">
            <DefaultState val="Shown"/>
            <State name="Shown">
                <Action type="SetProperty" PositionFixed="false"/>
            </State>
            <State name="Hidden">
                <When type="Property" frame="Label" Text=""/>
                <Action type="SetProperty" frame="Label" Visible="False"/>
                <Action type="SetProperty" frame="Label" Visible="true" on="Exit"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="BackgroundImage">
            <Anchor relative="$parent" offset="-5"/>
            <Texture val="@UI/Glues/TooltipBackground_Terr"/>
            <TextureType val="NineSlice"/>
            <Visible val="{$parent/Label/@Visible}"/>
        </Frame>

        <Frame type="Label" name="Label">
            <Style val="@KLC_AchievementDescription_Terr"/>
        </Frame>
    </Frame>

    <!-- Shared Dummy Info Frame -->

    <Frame type="Frame" name="AchievementDummyInfoFrameTemplate">
        <Anchor relative="$parent"/>
        
        <Alpha val="0"/>
        <Visible val="False"/>
        
        <Frame type="Image" name="AchievementIcon">
            <Anchor relative="$parent"/>
        </Frame>
        
        <Frame type="Label" name="AchievementTitle">
            <Anchor relative="$parent"/>
        </Frame>
        
        <Frame type="Label" name="AchievementDescription">
            <Anchor relative="$parent"/>
        </Frame>

        <Frame type="Label" name="AchievementPoints">
            <Anchor relative="$parent"/>
         </Frame>
        
        <Frame type="Label" name="CompletionDate">
            <Anchor relative="$parent"/>
        </Frame>
    </Frame>

    <!-- Green Border -->
    
    <Frame type="Frame" name="StandardRaceContainer">

		<StateGroup name="RaceStyle">
			<DefaultState val="Terran"/>

			<State name="Terran">
                <!--When type="StateGroup" frame="$this" RaceStyle="Terran"/-->
			</State>

            <State name="Zerg">
                <!--When type="StateGroup" frame="$this" RaceStyle="Zerg"/-->

                <Action type="SetProperty" frame="$this/InnerFrame" Texture="@UI/Glues/InnerFrame_Zerg"/>
                <Action type="SetProperty" frame="$this/OuterFrame" AdjustmentColor="#KL_DarkZerg"/>
			</State>

            <State name="Protoss">
                <!--When type="StateGroup" frame="$this" RaceStyle="Protoss"/-->

                <Action type="SetProperty" frame="$this/InnerFrame" Texture="@UI/Glues/InnerFrame_Prot"/>
                <Action type="SetProperty" frame="$this/OuterFrame" AdjustmentColor="#KL_DarkProtoss"/>
			</State>
		</StateGroup>

        <Frame type="Image" name="OuterFrame">
            <Anchor relative="$parent"/>

            <Texture val="@UI/Glues/StandardButtonBorderless_Background"/>
            <TextureType val="NineSlice"/>
            <AdjustmentColor val="#KL_DarkTerran"/>
            <ColorAdjustMode val="Fill"/>
        </Frame>

        <Frame type="Image" name="InnerFrame">
            <Anchor relative="$parent" offset="4"/>

            <Texture val="@UI/Glues/InnerFrameBorder_Terr"/>
            <TextureType val="NineSlice"/>
            <Unclipped val="True"/>
        </Frame>
    </Frame>

    <!-- Progress Bar Template -->

    <Frame type="Image" name="ProgressBarBackgroundTemplate">
        <Texture val="@UI/GameUI/ProgresBarBackground"/>
        <TextureType val="NineSlice"/>
    </Frame>

    <Frame type="ProgressBar" name="StandardProgressBarTemplate">
        <AcceptsMouse val="False"/>
        <MinValue val="0"/>
        <MaxValue val="1"/>
        <Value val="0"/>
        <UseFillContainer val="True"/>

        <Frame type="Frame" name="FillImageContainer">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="FillImage">
                <Anchor relative="$parent"/>

                <Texture val="@UI/GameUI/ProgressBarFill_Terr"/>
                <TextureType val="NineSlice"/>
                <SubpixelRendering val="True"/>
            </Frame>
        </Frame>
    </Frame>

    <!-- Unit Model Frame Template -->

    <Frame type="UnitModelFrame" name="UnitFrameTemplate">
        <Anchor relative="$parent"/>

        <TurningEnabled val="True"/>

        <Frame type="Model" name="Model">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <UseAlternateTime val="false"/>
            <UseAspectScaling val="false"/>
        </Frame>

        <UnitWorldX val="{$this/CombinedXOffset/@Result}"/>
        <UnitWorldY val="{$this/CombinedYOffset/@Result}"/>
        <UnitWorldZ val="{$this/CombinedZOffset/@Result}"/>

        <Frame type="MathFrame" name="CombinedXOffset">
            <A val="0"/>
            <B val="0"/>
            <Operator val="Add"/>
        </Frame>
        
        <Frame type="MathFrame" name="CombinedYOffset">
            <A val="0"/>
            <B val="0"/>
            <Operator val="Add"/>
        </Frame>
        
        <Frame type="MathFrame" name="CombinedZOffset">
            <A val="0"/>
            <B val="0"/>
            <Operator val="Add"/>
        </Frame>

