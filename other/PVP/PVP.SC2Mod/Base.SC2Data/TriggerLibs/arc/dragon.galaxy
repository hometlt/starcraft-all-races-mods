            if (AITechCount(player, c_DRAGU_COMBAT_2, c_techCountInProgressOrBetter) >= 6) {
                AISetStock( player, 1, c_DRAGR_UPGRADE_15 );
            }

            AISetStock( player, 1, c_DRAGR_LightWeapons1 );
            AISetStock( player, 1, c_DRAGR_GroundArmor1 );

            if (phase >= c_ldPhaseLate) {

                AISetStock( player, 1, c_DRAGR_LightWeapons2 );
                AISetStock( player, 1, c_DRAGR_GroundArmor2 );
                AISetStock( player, 1, c_DRAGR_HeavyWeapons1 );

                if (AITechCount(player, c_DRAGU_COMBAT_3, c_techCountInProgressOrBetter) >= 5) {
                    AISetStock( player, 1, c_DRAGR_UPGRADE_4 );
                    AISetStock( player, 1, c_DRAGR_UPGRADE_7 );
