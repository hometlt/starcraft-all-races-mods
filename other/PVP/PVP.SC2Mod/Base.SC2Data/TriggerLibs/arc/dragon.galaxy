                    AISetStock( player, 1, c_DRAGR_UPGRADE_14 );
                }
                if (AITechCount(player, c_DRAGU_TRANSPORT, c_techCountInProgressOrBetter) >= 2) {
                    AISetStock( player, 1, c_DRAGR_UPGRADE_13 );
                }

                AISetStock( player, 1, c_DRAGR_UPGRADE_2 );
                AISetStock( player, 1, c_DRAGR_UPGRADE_3 );
                AISetStock( player, 1, c_DRAGR_UPGRADE_12 );

                AISetStock( player, 1, c_DRAGR_AirWeapons1 );
