            AIAddToStockArmy(player, c_DRAGU_COMBAT_10, 2);
            AIAddToStockArmyPickFrom2Count(player, c_DRAGU_COMBAT_6, 2, c_DRAGU_COMBAT_10, 2);
            // validation: 3700 resources
            duration = 260 + RandomInt(-15, 15);
        } else if (armyRoll <= 4) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_3, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 3);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_6, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_8, 4);
            // validation: 4475 resources
            duration = 280 + RandomInt(-15, 15);
        } else {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_3, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_5, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_2, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_3, 2);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_4, 2);
            // validation: 4350  resources
            duration = 290 + RandomInt(-15, 15);
        }
    } else if (buildType == e_buildType_Special) {
        armyRoll = RandomInt(1,2);
        if (armyRoll <= 1) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_1, 12);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_4, 10);
            AIAddToStockArmyPickFrom2Count(player, c_DRAGU_COMBAT_3, 4, c_DRAGU_COMBAT_2, 3);
            AIAddToStockArmyPickFrom2Count(player, c_DRAGU_COMBAT_3, 4, c_DRAGU_COMBAT_2, 3);
            // validation: 5000 resources
            duration = 320 + RandomInt(-15, 15);
        } else if (armyRoll <= 2) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_1, 8);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_6, 4);
            AIAddToStockArmy(player, AIPickFrom2(c_DRAGU_COMBAT_9, c_DRAGU_COMBAT_AIR_4), 5);
            // validation: 4500 resources
            duration = 290 + RandomInt(-15, 15);
        }
    }
    else if (buildType == e_buildType_Mixed) {
        armyRoll = RandomInt(1,5);
        if (armyRoll <= 2) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_1, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_3, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_5, 5);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_4, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_9, 3);
            // validation: 5075 resources
            duration = 330 + RandomInt(-15, 15);
        } else if (armyRoll <= 4) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_1, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_5, 6);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_10, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_6, 4);
            // validation: 5050 resources
            duration = 320 + RandomInt(-15, 15);
        } else if (armyRoll <= 5) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_2, 8);
            AIAddToStockArmyPickFrom2Count(player, c_DRAGU_COMBAT_3, 7, c_DRAGU_COMBAT_5, 5);
            AIAddToStockArmy(player, AIPickFrom2(c_DRAGU_COMBAT_AIR_3, c_DRAGU_COMBAT_AIR_4), 3);
            AIAddToStockArmy(player, AIPickFrom2(c_DRAGU_COMBAT_AIR_5, c_DRAGU_COMBAT_8), 5);
            // validation: 5575 resources
            duration = 340 + RandomInt(-15, 15);
        }
    } else if (buildType == e_buildType_Air) {
        armyRoll = RandomInt(1,3);
        if (armyRoll <= 1) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_1, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_2, 7);
            AIAddToStockArmy(player, AIPickFrom2(c_DRAGU_COMBAT_AIR_1, c_DRAGU_COMBAT_AIR_2), 5);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_4, 2);
            // validation: 4750 resources
            duration = 290 + RandomInt(-15, 15);
        } else if (armyRoll <= 2) {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_1, 2);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_3, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_5, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_4, 2);
            // validation: 5250 resources
            duration = 300 + RandomInt(-15, 15);
        } else {
            AIAddToStockArmy(player, c_DRAGU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_2, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_1, 4);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_3, 5);
            AIAddToStockArmy(player, c_DRAGU_COMBAT_AIR_4, 2);
            // validation: 4850 resources
            duration = 310 + RandomInt(-15, 15);
        }
    } else {
        return DragonFinalArmyInitHard(player, e_buildType_Core);
    }

    AISetUserInt(player, c_ldNextArmyRoll, armyRoll);
    return duration;
}

//--------------------------------------------------------------------------------------------------
//  DragonLateInitHard
//--------------------------------------------------------------------------------------------------
void DragonLateInitHard (int player) {
    int lateLoop = AIGetUserInt(player, c_ldNumLateLoops);
    int curPhase = c_ldPhaseLate;
    int buildType = 0;

    AIChooseNextLateGameBuild(player);
    buildType = AIGetUserInt(player, c_lateGameBuildType);

    if (lateLoop <= 0) { // First Late Loop
        AISetupNextArmy (player, buildType, curPhase, DragonLateArmyInitHard);
    } else {
        curPhase = c_ldPhaseFinal + lateLoop;
        AISetupNextArmy (player, buildType, curPhase, DragonFinalArmyInitHard);
    }

    AISetFlag(player, e_flagsScouting, true);
    AISetFlag(player, e_flagsClearObs, true);
    AISetFlag(player, e_flagsLateScout, true);
    AISetFlag(player, e_flagsDetect, true);

    AISetMainState(player, e_mainState_Late, e_mainSubState_Build);
}

//--------------------------------------------------------------------------------------------------
//  DragonLateBuildHard
//--------------------------------------------------------------------------------------------------
void DragonLateBuildHard (int player) {
    int lateLoop = AIGetUserInt(player, c_ldNumLateLoops);
    int buildType = AIGetUserInt(player, c_lateGameBuildType);
    int armyRoll = AIGetUserInt(player, c_ldNextArmyRoll);
    int timeOut = AIGetUserInt(player, c_nextArmyTime);
    bool nextState = false;

    if (AIHasPlayerChangedCurrentLateGame(player)) {
        AISetMainState(player, e_mainState_Late, e_mainSubState_Init);
        return;
    }

    AIClearStock(player);

    DragonBasicEconomy(player, c_ldPhaseLate, 20);
    if (buildType == e_buildType_Air) {
        AISetStock( player, 1, c_DRAGU_TRANSPORT );
    }
    DragonBasicExpansion(player, c_ldPhaseLate, 14000, 4500);

    DragonBasicDefenseStock(player);

    AISetStockArmyScale(player, 0.35);

    DragonGeneralBuild(player, c_ldPhaseLate);

    AILDStockWorkers( player, 30, c_DRAGU_WORKER);

    if (buildType == e_buildType_Core) {
        if (DragonMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_DRAGB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_DRAGB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_HighTech) {
        if (DragonMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_DRAGB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_DRAGB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Special) {
        if (DragonMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_DRAGB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_DRAGB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Mixed) {
        if (DragonMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_DRAGB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_DRAGB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Air) {
        if (DragonMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 1, c_DRAGB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 2, c_DRAGB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_DRAGB_ADDON, c_stockIdle );
    }

    AISetStock( player, 2, c_DRAGU_DETECTOR );

    AISetStockArmyScale(player, 0.70);

    AISetStock( player, 2, c_DRAGU_TRANSPORT );
    AISetStock( player, 3, c_DRAGU_DETECTOR );
    AISetStock( player, 2, c_DRAGB_DEFENSE_2 );

    AISetStockArmyScale(player, 1.00);

    AILDStockWorkers( player, 60, c_DRAGU_WORKER);

    if (lateLoop <= 0) { // First Late Loop
        DragonUpgradesHard(player, c_ldPhaseLate);
        DragonTechUp(player, c_ldPhaseLate);
    } else { // Remaining Loops
        DragonUpgradesHard(player, c_ldPhaseFinal);
        DragonTechUp(player, c_ldPhaseFinal);
    }

    AIEnableStock(player);

    AILDGeneralSharedLogic(player);

    nextState = AITestTimeout(player, timeOut, e_mainState_Late, e_mainSubState_Init);
    if (nextState) {
        lateLoop += 1;
        AISetUserInt(player, c_ldNumLateLoops, lateLoop);
    }
}

//--------------------------------------------------------------------------------------------------
//  DragonOpenHard
//--------------------------------------------------------------------------------------------------
void DragonOpenHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        DragonOpenInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { DragonOpenBuildHard(player); }
    else { ErrorMeleeScript(player, "Invalid Open MainSubState DragonHard"); }
}

//--------------------------------------------------------------------------------------------------
//  DragonMidHard
//--------------------------------------------------------------------------------------------------
void DragonMidHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        DragonMidInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { DragonMidBuildHard(player);  }
    else { ErrorMeleeScript(player, "Invalid Mid MainSubState DragonHard"); }
}

//--------------------------------------------------------------------------------------------------
//  DragonLateHard
//--------------------------------------------------------------------------------------------------
void DragonLateHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        DragonLateInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { DragonLateBuildHard(player);  }
    else { ErrorMeleeScript(player, "Invalid Late MainSubState DragonHard"); }
}



