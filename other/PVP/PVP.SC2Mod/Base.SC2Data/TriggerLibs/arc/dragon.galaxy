void DragonBasicDefenseStock (int player) {
    if (!AINeedsDefending(player)) {
        return;
    }

    AISetStockFarms( player, c_DRAGB_SUPPLY, c_stockNormalFarms);
    AIAddEmergencyDefenseStock(player, 4, c_DRAGU_COMBAT_1, false);
    AISetStock( player, 1, c_DRAGB_REFINERY );
    AIAddEmergencyDefenseStock(player, 4, c_DRAGU_COMBAT_AIR_2, true);
    AIAddEmergencyDefenseStock(player, 2, c_DRAGU_COMBAT_7, true);
    AIAddEmergencyDefenseStock(player, 4, c_DRAGU_COMBAT_2, true);
    AIAddEmergencyDefenseStock(player, 20, c_DRAGU_WORKER, true);
}

