                    }
                    if (AITechCount(player, c_DRAGU_COMBAT_AIR_1, c_techCountInProgressOrBetter) >= 4) {
                        AISetStock( player, 1, c_DRAGR_UPGRADE_11 );
                    }
                    if (AITechCount(player, c_DRAGU_COMBAT_AIR_3, c_techCountInProgressOrBetter) >= 3) {
                        AISetStock( player, 1, c_DRAGR_UPGRADE_9 );
                    }
                    if (AITechCount(player, c_DRAGU_COMBAT_AIR_5, c_techCountInProgressOrBetter) >= 2) {
                        AISetStock( player, 1, c_DRAGR_UPGRADE_16 );
                    }

                    AISetStock( player, 1, c_DRAGR_LightWeapons3 );
                    AISetStock( player, 1, c_DRAGR_GroundArmor3 );
