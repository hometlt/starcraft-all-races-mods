            AIAddToStockArmyPickFrom2Count(player, c_NHBRU_COMBAT_3, 4, c_NHBRU_COMBAT_2, 3);
            // validation: 5000 resources
            duration = 320 + RandomInt(-15, 15);
        } else if (armyRoll <= 2) {
