            AIAddToStockArmy(player, c_NHBRU_COMBAT_3, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_5, 5);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_4, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_9, 3);
            // validation: 5075 resources
            duration = 330 + RandomInt(-15, 15);
        } else if (armyRoll <= 4) {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_1, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_2, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_5, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_10, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_6, 4);
            // validation: 5050 resources
            duration = 320 + RandomInt(-15, 15);
        } else if (armyRoll <= 5) {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_2, 8);
            AIAddToStockArmyPickFrom2Count(player, c_NHBRU_COMBAT_3, 7, c_NHBRU_COMBAT_5, 5);
            AIAddToStockArmy(player, AIPickFrom2(c_NHBRU_COMBAT_AIR_3, c_NHBRU_COMBAT_AIR_4), 3);
