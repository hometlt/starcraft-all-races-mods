            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_5, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_4, 2);
            // validation: 5250 resources
            duration = 300 + RandomInt(-15, 15);
        } else {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_2, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_1, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_3, 5);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_4, 2);
            // validation: 4850 resources
            duration = 310 + RandomInt(-15, 15);
        }
    } else {
        return HybridFinalArmyInitHard(player, e_buildType_Core);
    }

    AISetUserInt(player, c_ldNextArmyRoll, armyRoll);
    return duration;
}

//--------------------------------------------------------------------------------------------------
//  HybridLateInitHard
//--------------------------------------------------------------------------------------------------
void HybridLateInitHard (int player) {
    int lateLoop = AIGetUserInt(player, c_ldNumLateLoops);
    int curPhase = c_ldPhaseLate;
    int buildType = 0;

    AIChooseNextLateGameBuild(player);
    buildType = AIGetUserInt(player, c_lateGameBuildType);

    if (lateLoop <= 0) { // First Late Loop
        AISetupNextArmy (player, buildType, curPhase, HybridLateArmyInitHard);
    } else {
        curPhase = c_ldPhaseFinal + lateLoop;
        AISetupNextArmy (player, buildType, curPhase, HybridFinalArmyInitHard);
    }

    AISetFlag(player, e_flagsScouting, true);
    AISetFlag(player, e_flagsClearObs, true);
    AISetFlag(player, e_flagsLateScout, true);
    AISetFlag(player, e_flagsDetect, true);

    AISetMainState(player, e_mainState_Late, e_mainSubState_Build);
}

//--------------------------------------------------------------------------------------------------
//  HybridLateBuildHard
//--------------------------------------------------------------------------------------------------
void HybridLateBuildHard (int player) {
    int lateLoop = AIGetUserInt(player, c_ldNumLateLoops);
    int buildType = AIGetUserInt(player, c_lateGameBuildType);
    int armyRoll = AIGetUserInt(player, c_ldNextArmyRoll);
    int timeOut = AIGetUserInt(player, c_nextArmyTime);
    bool nextState = false;

    if (AIHasPlayerChangedCurrentLateGame(player)) {
        AISetMainState(player, e_mainState_Late, e_mainSubState_Init);
        return;
    }

    AIClearStock(player);

    HybridBasicEconomy(player, c_ldPhaseLate, 20);
    if (buildType == e_buildType_Air) {
        AISetStock( player, 1, c_NHBRU_TRANSPORT );
    }
    HybridBasicExpansion(player, c_ldPhaseLate, 14000, 4500);

    HybridBasicDefenseStock(player);

    AISetStockArmyScale(player, 0.35);

    HybridGeneralBuild(player, c_ldPhaseLate);

    AILDStockWorkers( player, 30, c_NHBRU_WORKER);

    if (buildType == e_buildType_Core) {
        if (HybridMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_NHBRB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_NHBRB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_HighTech) {
        if (HybridMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_NHBRB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_NHBRB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Special) {
        if (HybridMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_NHBRB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_NHBRB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Mixed) {
        if (HybridMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 1, c_NHBRB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_NHBRB_ADDON, c_stockIdle );
    }
    else if (buildType == e_buildType_Air) {
        if (HybridMacroExpand(player)) { return; }
        AISetStockUnitNext( player, 1, c_NHBRB_PROD_1, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_2, c_stockIdle );
        AISetStockUnitNext( player, 2, c_NHBRB_PROD_3, c_stockIdle );
        AISetStockUnitNext( player, 15, c_NHBRB_ADDON, c_stockIdle );
    }

    AISetStock( player, 2, c_NHBRU_DETECTOR );

    AISetStockArmyScale(player, 0.70);

    AISetStock( player, 2, c_NHBRU_TRANSPORT );
    AISetStock( player, 3, c_NHBRU_DETECTOR );
    AISetStock( player, 2, c_NHBRB_DEFENSE_2 );

    AISetStockArmyScale(player, 1.00);

    AILDStockWorkers( player, 60, c_NHBRU_WORKER);

    if (lateLoop <= 0) { // First Late Loop
        HybridUpgradesHard(player, c_ldPhaseLate);
        HybridTechUp(player, c_ldPhaseLate);
    } else { // Remaining Loops
        HybridUpgradesHard(player, c_ldPhaseFinal);
        HybridTechUp(player, c_ldPhaseFinal);
    }

    AIEnableStock(player);

    AILDGeneralSharedLogic(player);

    nextState = AITestTimeout(player, timeOut, e_mainState_Late, e_mainSubState_Init);
    if (nextState) {
        lateLoop += 1;
        AISetUserInt(player, c_ldNumLateLoops, lateLoop);
    }
}

//--------------------------------------------------------------------------------------------------
//  HybridOpenHard
//--------------------------------------------------------------------------------------------------
void HybridOpenHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        HybridOpenInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { HybridOpenBuildHard(player); }
    else { ErrorMeleeScript(player, "Invalid Open MainSubState HybridHard"); }
}

//--------------------------------------------------------------------------------------------------
//  HybridMidHard
//--------------------------------------------------------------------------------------------------
void HybridMidHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        HybridMidInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { HybridMidBuildHard(player);  }
    else { ErrorMeleeScript(player, "Invalid Mid MainSubState HybridHard"); }
}

//--------------------------------------------------------------------------------------------------
//  HybridLateHard
//--------------------------------------------------------------------------------------------------
void HybridLateHard (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (mainSubState != e_mainSubState_Build) {
        HybridLateInitHard(player);
        mainSubState = AIState(player, e_mainSubState);
    }

    if (mainSubState == e_mainSubState_Build) { HybridLateBuildHard(player);  }
    else { ErrorMeleeScript(player, "Invalid Late MainSubState HybridHard"); }
}



