            AIAddToStockArmyPickFrom2Count(player, c_NHBRU_COMBAT_6, 2, c_NHBRU_COMBAT_10, 2);
            // validation: 3700 resources
            duration = 260 + RandomInt(-15, 15);
        } else if (armyRoll <= 4) {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_3, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_2, 3);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_6, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_8, 4);
            // validation: 4475 resources
            duration = 280 + RandomInt(-15, 15);
        } else {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_3, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_2, 6);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_5, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_2, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_3, 2);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_4, 2);
            // validation: 4350  resources
            duration = 290 + RandomInt(-15, 15);
        }
    } else if (buildType == e_buildType_Special) {
        armyRoll = RandomInt(1,2);
        if (armyRoll <= 1) {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_1, 12);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_4, 10);
