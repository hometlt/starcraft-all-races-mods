        }
    } else if (buildType == e_buildType_Air) {
        armyRoll = RandomInt(1,3);
        if (armyRoll <= 1) {
            AIAddToStockArmy(player, c_NHBRU_COMBAT_7, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_1, 4);
            AIAddToStockArmy(player, c_NHBRU_COMBAT_AIR_2, 7);
            AIAddToStockArmy(player, AIPickFrom2(c_NHBRU_COMBAT_AIR_1, c_NHBRU_COMBAT_AIR_2), 5);
