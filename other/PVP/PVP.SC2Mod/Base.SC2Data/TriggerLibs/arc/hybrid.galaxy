    AIAddEmergencyDefenseStock(player, 2, c_NHBRU_COMBAT_7, true);
    AIAddEmergencyDefenseStock(player, 4, c_NHBRU_COMBAT_2, true);
    AIAddEmergencyDefenseStock(player, 20, c_NHBRU_WORKER, true);
}

