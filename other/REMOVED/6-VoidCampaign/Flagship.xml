    <CActorTurret id="FlagshipRotateTurret">
        <On Terms="TurretEnable.FlagshipRotate" Send="Create"/>
    </CActorTurret>
    <CActorModel id="FlagshipDeathModel" parent="UnitDeathModel">
        <HostSiteOps Ops="SOpAttachOrigin"/>
    </CActorModel>
    <CActorBeamSimple id="FlagshipAttackBeam" parent="BeamSimpleAnimationStyleContinuous">
        <On Terms="Effect.FlagshipBeamPersistent.Stop" Send="AnimBracketStop BSD"/>
        <Macros value="ActionTargetTeleportDestroy"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="FlagshipSecondaryAttackBeam" parent="BeamSimpleAnimationStyleContinuous">
        <Model value="FlagshipAttackBeam"/>
        <On Terms="Effect.FlagshipSecondaryBeamPersistent.Stop" Send="AnimBracketStop BSD"/>
        <Macros value="ActionTargetTeleportDestroy"/>
    </CActorBeamSimple>
    <CActorAction id="FlagshipAttack" parent="GenericAttackBase">
        <LaunchAttachQuery Methods="AMFilterWeaponSetA0 AMClosest2ToTarget AMPatternFlagshipBeam"/>
        <LaunchAssets Sound="Mothership_AttackLaunch"/>
        <ActionFlags index="AlignedImpactsAndDamages" value="1"/>
        <ActionFlags index="ImpactMonitorsTeleports" value="1"/>
        <Lifetime value="-1.000000"/>
        <On Terms="Effect.FlagshipBeamPersistent.Start" Send="Create"/>
        <On Terms="Effect.FlagshipBeamDamage.Start; FromEffectTreeDescendant" Send="ActionDamage"/>
        <!-- Need to delay the attack long enough to be able kill a beam during its death anim, when
             the target teleports. -->
        <On Terms="Effect.FlagshipBeamPersistent.Stop" Send="Destroy WhenReady"/>
    </CActorAction>
    <CActorAction id="FlagshipSecondaryAttack" parent="GenericAttackBase">
        <LaunchAttachQuery Methods="AMFilterWeaponSetA0 AMClosest2ToTarget AMPatternFlagshipBeam"/>
        <ActionFlags index="AlignedImpactsAndDamages" value="1"/>
        <ActionFlags index="ImpactMonitorsTeleports" value="1"/>
        <Lifetime value="-1.000000"/>
        <On Terms="Effect.FlagshipSecondaryBeamPersistent.Start" Send="Create"/>
        <On Terms="Effect.FlagshipBeamDamage.Start; FromEffectTreeDescendant" Send="ActionDamage"/>
        <!-- Need to delay the attack long enough to be able kill a beam during its death anim, when
             the target teleports. -->
        <On Terms="Effect.FlagshipBeamPersistent.Stop" Send="Destroy WhenReady"/>
    </CActorAction>
    <CActorSound id="FlagshipAttackImpact" parent="SoundContinuous">
        <Sound value="Mothership_AttackImpact"/>
        <On Terms="Effect.FlagshipBeamDamage.Start; EffectExecuteIndex 1" Send="Create"/>
        <On Terms="Effect.FlagshipBeamPersistent.Stop" Send="Destroy"/>
    </CActorSound>
    <CActorSimple id="FlagshipWarpInSimple">
        <On Terms="Behavior.FlagshipWarpIn.On" Send="Create"/>
        <On Terms="ActorCreation" Target="_Unit" Send="SetOpacity 0.400000 0.000000 FlagshipWarpInOpacity"/>
        <On Terms="ActorCreation" Target="_Unit" Send="SetTintColor 100,100,255 0.000000 OneShot FlagshipWarpInTint"/>
        <On Terms="Behavior.FlagshipWarpIn.Off" Target="_Unit" Send="ClearOpacity 0.250000 FlagshipWarpInOpacity"/>
        <On Terms="Behavior.FlagshipWarpIn.Off" Target="_Unit" Send="ClearTintColor 0.250000 FlagshipWarpInTint"/>
        <On Terms="Behavior.FlagshipWarpIn.Off" Send="Destroy"/>
    </CActorSimple>
    <CActorModel id="FlagshipWarpInImpact" parent="ModelAnimationStyleOneShot">
        <On Terms="Behavior.FlagshipWarpIn.On" Send="Create"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachOrigin"/>
        <Model value="ArbiterMPRecalledImpact"/>
        <AutoScaleFactor value="0.450000"/>
    </CActorModel>
    <CActorSound id="FlagshipWarpInImpactSound" parent="SoundOneShot">
        <On Terms="Behavior.FlagshipWarpIn.On" Send="Create"/>
        <Host Subject="_Selectable"/>
        <Sound value="ArbiterMP_RecallEnd"/>
    </CActorSound>
