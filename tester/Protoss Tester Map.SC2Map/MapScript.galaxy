bool gt_Pause_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!(gf_AttackMode())) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((gv_pause == false)) {
        UIDisplayMessage(PlayerGroupActive(), c_messageAreaDirective, StringExternal("Param/Value/3C205BF3"));
        UnitPauseAll(true);
        gv_pause = true;
        gf_HighlightButton(gv_buttonPause, true);
