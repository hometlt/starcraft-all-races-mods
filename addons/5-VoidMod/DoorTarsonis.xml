    <CActorUnit default="1" id="Tarsonis_Door" parent="GenericUnitStandardNoAutoSoundLinks">
        <?token id="alt" value="Lowered"?>
        <DeathArray index="Normal" ModelLink="TerranBuildingDeath" SoundLink="Terran_ExplosionLarge"/>
        <On Terms="UnitBirth.##unitName####alt##" Send="Create"/>
        <On Terms="UnitBirth.##unitName####alt##" Send="AnimGroupApply Unpowered Unpowered,Start Unpowered,End Instant"/>
        <!-- Become unpowered. -->
        <On Terms="AbilMorph.*.Start; MorphTo ##unitName####alt##" Send="AnimGroupApply Unpowered Unpowered,Start Unpowered,End"/>
        <On Terms="AbilMorph.*.Finish; MorphTo ##unitName####alt##" Send="AnimGroupApply Unpowered Unpowered,Start Unpowered,End Instant"/>
        <!-- Become powered. -->
        <On Terms="AbilMorph.*.Start; MorphTo ##unitName##" Send="AnimGroupRemove Unpowered"/>
        <On Terms="AbilMorph.*.Finish; MorphTo ##unitName##" Send="AnimGroupRemove Unpowered Instant"/>
        <Model value="Tarsonis_Door"/>
        <UnitFlags index="ShowBlobShadow" value="0"/>
    </CActorUnit>
