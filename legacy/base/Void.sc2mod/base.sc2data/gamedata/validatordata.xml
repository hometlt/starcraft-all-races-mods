<Catalog>

    <CValidatorUnitFilters id="AirUnitFilter">
        <Filters value="Air;Stasis,Dead,Hidden,Invulnerable,Unstoppable"/>
    </CValidatorUnitFilters>
    <CValidatorUnitFilters id="GroundUnitFilter">
        <Filters value="Ground;Stasis,Dead,Hidden,Invulnerable,Unstoppable"/>
    </CValidatorUnitFilters>
    <CValidatorUnitFilters id="TargetIsPlayer">
        <Filters value="-;Ally,Neutral,Enemy"/>
    </CValidatorUnitFilters>
    <CValidatorUnitOrderQueue id="CasterIsMoving">
        <AbilLink value="move"/>
    </CValidatorUnitOrderQueue>
    <CValidatorCombine id="CasterIsMovingOrStopped">
        <CombineArray value="CasterIsMoving"/>
        <CombineArray value="CasterIsStopped"/>
        <CombineArray value="CasterHasNoOrders"/>
    </CValidatorCombine>
    <CValidatorUnitOrderQueue id="CasterIsStopped">
        <AbilLink value="stop"/>
    </CValidatorUnitOrderQueue>

    <!-- Food use validators for other effects, these all check a range so for example a zergling will return in the 1 category -->
    <CValidatorCombine id="FoodUse1">
        <Type value="And"/>
        <CombineArray value="FoodUse1Low"/>
        <CombineArray value="FoodUse1High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse1High">
        <Compare value="LT"/>
        <Field value="Food"/>
        <Value value="0"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse1Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-1.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorCombine id="FoodUse2">
        <Type value="And"/>
        <CombineArray value="FoodUse2Low"/>
        <CombineArray value="FoodUse2High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse2High">
        <Compare value="LE"/>
        <Field value="Food"/>
        <Value value="-1.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse2Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-2.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorCombine id="FoodUse3">
        <Type value="And"/>
        <CombineArray value="FoodUse3Low"/>
        <CombineArray value="FoodUse3High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse3High">
        <Compare value="LE"/>
        <Field value="Food"/>
        <Value value="-2.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse3Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-3.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorCombine id="FoodUse4">
        <Type value="And"/>
        <CombineArray value="FoodUse4Low"/>
        <CombineArray value="FoodUse4High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse4High">
        <Compare value="LE"/>
        <Field value="Food"/>
        <Value value="-3.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse4Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-4.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorCombine id="FoodUse5">
        <Type value="And"/>
        <CombineArray value="FoodUse5Low"/>
        <CombineArray value="FoodUse5High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse5High">
        <Compare value="LE"/>
        <Field value="Food"/>
        <Value value="-4.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse5Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-5.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorCombine id="FoodUse6">
        <Type value="And"/>
        <CombineArray value="FoodUse6Low"/>
        <CombineArray value="FoodUse6High"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="FoodUse6High">
        <Compare value="LE"/>
        <Field value="Food"/>
        <Value value="-5.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="FoodUse6Low">
        <Compare value="GT"/>
        <Field value="Food"/>
        <Value value="-6.5"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareField>

    <CValidatorPlayerRequirement id="HaveCinematicMode">
        <WhichPlayer Value="Caster"/>
        <Find value="1"/>
        <Value value="HaveCinematicMode"/>
    </CValidatorPlayerRequirement>
    <CValidatorPlayerRequirement id="HaveCursorDebug">
        <WhichPlayer Value="Caster"/>
        <Find value="1"/>
        <Value value="HaveCursorDebug"/>
    </CValidatorPlayerRequirement>
    <CValidatorUnitDetected id="IsDetected">
        <WhichPlayer Value="Caster"/>
    </CValidatorUnitDetected>
    <CValidatorUnitFilters id="IsMechanical">
        <Filters value="Mechanical;-"/>
    </CValidatorUnitFilters>
    <CValidatorUnitFilters id="IsNotFriendly">
        <Filters value="-;Self,Player,Ally"/>
    </CValidatorUnitFilters>
    <CValidatorCombine id="Minerals">
        <CombineArray value="PurifierMineralField"/>
        <CombineArray value="PurifierMineralField750"/>
        <CombineArray value="PurifierRichMineralField"/>
        <CombineArray value="PurifierRichMineralField750"/>
    </CValidatorCombine>
    <CValidatorCombine id="NotDeadorCloakedorBuried">
        <CombineArray value="NotDead"/>
        <CombineArray value="NotCloaked"/>
        <CombineArray value="IsNotBuried"/>
    </CValidatorCombine>
    <CValidatorUnitType id="PurifierMineralField">
        <Value value="PurifierMineralField"/>
    </CValidatorUnitType>
    <CValidatorUnitType id="PurifierRichMineralField">
        <Value value="PurifierRichMineralField"/>
    </CValidatorUnitType>
    <CValidatorUnitType id="PurifierMineralField750">
        <Value value="PurifierMineralField750"/>
    </CValidatorUnitType>
    <CValidatorUnitType id="PurifierRichMineralField750">
        <Value value="PurifierRichMineralField750"/>
    </CValidatorUnitType>
    <CValidatorUnitCompareSpeed id="SourceIsNotStationary">
        <Compare value="NE"/>
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareSpeed>
    <CValidatorUnitCompareSpeed id="SourceIsStationary">
        <WhichUnit Value="Source"/>
    </CValidatorUnitCompareSpeed>
    <CValidatorLocationCompareRange id="TargetLE2Range">
        <WhichLocation Value="TargetUnit"/>
        <Compare value="LT"/>
        <Range value="2"/>
    </CValidatorLocationCompareRange>

    <CValidatorUnitFilters id="NotHeroic">
        <Filters value="-;Heroic"/>
    </CValidatorUnitFilters>
    <CValidatorLocationCompareCliffLevel id="CliffLevelNot0">
        <Compare value="NE"/>
    </CValidatorLocationCompareCliffLevel>
    <CValidatorLocationCompareRange id="DistanceGreaterThan6">
        <WhichLocation Value="TargetUnitOrPoint"/>
        <Compare value="GT"/>
        <Range value="6"/>
    </CValidatorLocationCompareRange>
    <CValidatorUnitFilters id="CasterIsAlive">
        <WhichUnit Value="Caster"/>
        <Filters value="-;Dead"/>
    </CValidatorUnitFilters>



</Catalog>