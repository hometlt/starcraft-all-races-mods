<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CSkin default="1" id="TrophySkinBase">
        <ReplacementArray Catalog="Model" From="ClassicTrophyModel" To="##id##Model"/>
    </CSkin>
    <CSkin default="1" id="CollectionSkinBase">
        <?token id="unit" type="CUnitLink"?>
        <?token id="collection" value="Bogus"?>
        <?token id="race" value="Neutral"?>
        <Rotation value="35"/>
        <ReplacementArray Catalog="Model" From="##unit##" To="##unit##_##collection##"/>
        <ReplacementArray Catalog="Model" From="##unit##Death" To="##unit##_##collection##_Death"/>
        <ReplacementArray Catalog="Model" From="##unit##DeathRagdoll" To="##unit##_##collection##_DeathRagdoll"/>
        <ReplacementArray Catalog="Model" From="##unit##Portrait" To="##unit##_##collection##_Portrait"/>
        <CollectionDisplayUnit value="##unit##"/>
        <DisplayModel value="##unit##_##collection##"/>
        <WarChestUILight value="WarChestSkinPreview_##collection##"/>
        <CollectionDisplayModel value="##unit##_##collection##"/>
        <VariationIcon value="Assets\Textures\btn-unit-##race##-##unit##-##collection##.dds"/>
    </CSkin>
</Catalog>
