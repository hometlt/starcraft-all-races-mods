<Catalog>
    <CActorUnit default="1" id="XelNagaTempleDoorLarge" parent="GenericUnitStandardNoAutoSoundLinks">
        <?token id="alt" value="Lowered"?>
        <On Terms="UnitBirth.##unitName####alt##" Send="Create"/>
        <On Terms="UnitBirth.##unitName####alt##" Send="AnimGroupApply Work Stand,Work,Start Stand,Work,End Instant"/>
        <!-- Become unpowered. -->
        <On Terms="AbilMorph.*.Start; MorphTo ##unitName####alt##" Send="AnimGroupApply Work Stand,Work,Start Stand,Work,End"/>
        <On Terms="AbilMorph.*.Finish; MorphTo ##unitName####alt##" Send="AnimGroupApply Work Stand,Work,Start Stand,Work,End Instant"/>
        <!-- Become powered. -->
        <On Terms="AbilMorph.*.Start; MorphTo ##unitName##" Send="AnimGroupRemove Work"/>
        <On Terms="AbilMorph.*.Finish; MorphTo ##unitName##" Send="AnimGroupRemove Work Instant"/>
        <Model value="XelNaga_Door"/>
        <DeathArray index="Normal" ModelLink="TerranBuildingDeath" SoundLink="Terran_ExplosionLarge"/>
        <UnitFlags index="ShowBlobShadow" value="0"/>
    </CActorUnit>
