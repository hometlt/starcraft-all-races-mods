<Catalog>

        <CActorUnit id="PylonContainmentUnit" parent="GenericUnitStandardNoAutoSoundLinks" unitName="PylonContainmentUnit">
            <On Terms="ActorCreation" Send="Create Pylon_Containment_Unit_Loop_ActorSound"/>
