<Catalog>
    <CActorModel default="1" id="FrozenProtossBuildingParent">
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
        <ModelFlags index="AllowHitTest" value="0"/>
        <ModelFlags index="UseSyncSeed" value="1"/>
        <Aliases value="_Snapshot"/>
        <Host Subject="_Selectable"/>
        <Inherits index="CastShadows" value="1"/>
        <Inherits index="CloakEffect" value="1"/>
        <Inherits index="Decal" value="1"/>
        <Inherits index="Opacity" value="1"/>
        <Inherits index="Scale" value="1"/>
        <Inherits index="TeamColor" value="1"/>
        <Inherits index="Visibility" value="1"/>
        <Inherits index="WarpGroup" value="1"/>
    </CActorModel>
    <!-- Expedition Protoss Frozen Solid                                            -->
    <CActorSimple id="FrozenSolidTinter">
        <On Terms="Behavior.FlashFreezeLevel1.On" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel2.On" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel1.On" Target="_Unit" Send="SetTintColor 150,150,255 0.250000 OneShot FrozenSolidTint"/>
        <On Terms="Behavior.FlashFreezeLevel2.On" Target="_Unit" Send="SetTintColor 100,100,255 0.250000 OneShot FrozenSolidTint"/>
        <On Terms="Behavior.FlashFreezeLevel1.Off" Target="_Unit" Send="ClearTintColor 0.250000 FrozenSolidTint"/>
        <On Terms="Behavior.FlashFreezeLevel2.Off" Target="_Unit" Send="ClearTintColor 0.250000 FrozenSolidTint"/>
        <On Terms="Behavior.FlashFreezeLevel1.Off" Send="Destroy"/>
        <On Terms="Behavior.FlashFreezeLevel2.Off" Send="Destroy"/>
    </CActorSimple>
    <CActorModel id="FrozenSolid" parent="ModelAddition">
        <On Terms="ActorCreation; ValidateUnit IsFrozenZergStructure" Send="Destroy"/>
        <On Terms="ActorCreation; ValidateUnit IsFrozenProtossStructure" Send="Destroy"/>
        <On Terms="ActorCreation; ValidateUnit IsColossus" Send="Destroy"/>
        <On Terms="ActorCreation; ValidateUnit IsFrozenProtossUnit" Send="Destroy"/>
        <On Terms="ActorCreation; ValidateUnit IsFrozenProtossAirUnit" Send="Destroy"/>
        <!-- WildMutationZergling checks vs all types of zergling -->
        <On Terms="ActorCreation; ValidateUnit WildMutationZergling" Send="SetScale 1.500000 0.500000"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimSetPausedAll"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Send="HostSiteOpsSet ::Host SOpAttachOrigin"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsRoach" Send="Destroy"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsOverlord" Send="Destroy"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsDrone" Send="Destroy"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPausedAll 0"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
        <AutoScaleFactor value="1.000000"/>
        <Inherits index="TimeScale" value="0"/>
        <Inherits index="TintColor" value="0"/>
        <ModelFlags index="AllowHitTest" value="0"/>
    </CActorModel>
    <CActorModel id="FrozenProtossColossus" parent="ModelAddition">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsColossus" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimClear PsionicLiftKnockback 0.000000"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimSetPausedAll"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Send="HostSiteOpsSet ::Host SOpAttachOrigin"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPausedAll 0"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
        <!--On Terms="Behavior.PsionicLiftRaise.On" Send="Destroy"/-->
        <Remove Terms="ActorOrphan" Send="Destroy"/>
        <AutoScaleFactor value="1.000000"/>
        <Inherits index="TimeScale" value="0"/>
        <Inherits index="TintColor" value="0"/>
        <ModelFlags index="AllowHitTest" value="0"/>
    </CActorModel>
    <CActorModel id="FrozenProtossAirUnit" parent="ModelAddition">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsFrozenProtossAirUnit" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimSetPausedAll"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Send="HostSiteOpsSet ::Host SOpAttachOrigin"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPausedAll 0"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
        <!--On Terms="Behavior.PsionicLiftRaise.On" Send="Destroy"/-->
        <On Terms="ActorCreation; ValidateUnit IsScout" Send="SetScale 0.700000"/>
        <On Terms="ActorCreation; ValidateUnit IsTempest" Send="SetScale 0.700000"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
        <AutoScaleFactor value="0.600000"/>
        <Inherits index="TimeScale" value="0"/>
        <Inherits index="TintColor" value="0"/>
        <ModelFlags index="AllowHitTest" value="0"/>
    </CActorModel>
    <CActorModel id="FrozenProtossUnit" parent="ModelAddition">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsFrozenProtossUnit" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimClear PsionicLiftKnockback 0.000000"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit NotRoachOverlordDrone" Target="_Unit" Send="AnimSetPausedAll"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Send="HostSiteOpsSet ::Host SOpAttachOrigin"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPausedAll 0"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
        <!--On Terms="Behavior.PsionicLiftRaise.On" Send="Destroy"/-->
        <Remove Terms="ActorOrphan" Send="Destroy"/>
        <AutoScaleFactor value="1.000000"/>
        <Inherits index="TimeScale" value="0"/>
        <Inherits index="TintColor" value="0"/>
        <ModelFlags index="AllowHitTest" value="0"/>
    </CActorModel>
    <CActorSimple id="FrozenZergRoach">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsRoach" Send="Create"/>
        <On Terms="ActorCreation" Target="_Unit" Send="ModelSwap FrozenZergRoach"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="$Spell PlayForever"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="TimerSet 2.200000 BreakOut"/>
        <On Terms="UnitDeath" Send="TimerKill BreakOut"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="ModelSwap RoachEx1"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="TimerExpired; TimerName BreakOut" Send="Destroy"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="ModelSwap RoachEx1"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
    </CActorSimple>
    <CActorSimple id="FrozenZergOverlord">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsOverlord" Send="Create"/>
        <On Terms="ActorCreation" Target="_Unit" Send="ModelSwap FrozenZergOverlord"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="$Spell PlayForever"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="TimerSet 3.340000 BreakOut"/>
        <On Terms="UnitDeath" Send="TimerKill BreakOut"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="ModelSwap Overlord"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="TimerExpired; TimerName BreakOut" Send="Destroy"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="ModelSwap Overlord"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
    </CActorSimple>
    <CActorSimple id="FrozenZergDrone">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsDrone" Send="Create"/>
        <On Terms="ActorCreation" Target="_Unit" Send="ModelSwap FrozenZergDrone"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="$Spell"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Send="TimerSet 3.330000 BreakOut"/>
        <On Terms="UnitDeath" Send="TimerKill BreakOut"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="ModelSwap Drone"/>
        <On Terms="TimerExpired; TimerName BreakOut" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="TimerExpired; TimerName BreakOut" Send="Destroy"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="ModelSwap Drone"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Target="_Unit" Send="AnimBaselineStart"/>
        <On Terms="Behavior.ThawUnitInstantly.Off" Send="Destroy"/>
    </CActorSimple>
    <CActorModel id="FrozenZergHatchery" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsHatcheryLairOrHive" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenZergSpawningPool" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsSpawningPool" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenZergExtractor" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsExtractor" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenZergEvolutionChamber" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsEvolutionChamber" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenZergSpineCrawler" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsSpineCrawler" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsSporeCrawler" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenZergRoachWarren" parent="FrozenZergBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsRoachWarren" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossAssimilator" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsAssimilator" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossCyberneticsCore" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsCyberneticsCore" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossForge" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsForge" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenRadioTower" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsProtossRadioTower" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Target="_Unit" Send="AnimSetPaused GL_BSD"/>
        <On Terms="Behavior.FlashFreezeLevel3.On" Target="_Unit" Send="AnimSetPaused ::Baseline"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPaused GL_BSD 0"/>
        <On Terms="Behavior.FlashFreezeLevel3.Off" Target="_Unit" Send="AnimSetPaused ::Baseline 0"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossGateway" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsGateway" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsWarpgate" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossStargate" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsStarGate" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossNexus" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsNexus" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossPhotonCannon" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsPhotonCannon" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossPylon" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsPylon" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FrozenProtossRoboticsFacility" parent="FrozenProtossBuildingParent">
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsRoboticsFacility" Send="Create"/>
        <On Terms="Behavior.FlashFreezeLevel3.On; ValidateUnit IsRoboticsBay" Send="Create"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
    </CActorModel>

    <CValidatorCombine id="IsFrozenProtossStructure">
        <CombineArray value="IsAssimilator"/>
        <CombineArray value="IsCyberneticsCore"/>
        <CombineArray value="IsForge"/>
        <CombineArray value="IsGateway"/>
        <CombineArray value="IsWarpgate"/>
        <CombineArray value="IsPhotonCannon"/>
        <CombineArray value="IsPylon"/>
        <CombineArray value="IsRoboticsFacility"/>
        <CombineArray value="IsRoboticsBay"/>
        <CombineArray value="IsProtossRadioTower"/>
        <CombineArray value="IsNexus"/>
        <CombineArray value="IsStarGate"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsFrozenProtossAirUnit">
        <CombineArray value="IsScout"/>
        <CombineArray value="IsPhoenix"/>
        <CombineArray value="IsVoidRay"/>
        <CombineArray value="IsCarrier"/>
        <CombineArray value="IsInterceptor"/>
        <CombineArray value="IsOracle"/>
        <CombineArray value="IsMothership"/>
        <CombineArray value="IsTempest"/>
        <CombineArray value="IsObserver"/>
        <CombineArray value="IsWarpPrism"/>
        <CombineArray value="IsWarpPrismPhasing"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsFrozenProtossUnit">
        <CombineArray value="IsProbe"/>
        <CombineArray value="IsZealot"/>
        <CombineArray value="IsArchon"/>
        <CombineArray value="IsSentry"/>
        <CombineArray value="IsStalker"/>
        <CombineArray value="IsHighTemplar"/>
        <CombineArray value="IsImmortal"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsFrozenProtossUnitVoid">
        <CombineArray value="IsProtossUnit"/>
        <CombineArray value="IsNotFlying"/>
        <Type value="And"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsFrozenProtossAirUnitVoid">
        <CombineArray value="IsProtoss"/>
        <CombineArray value="IsFlying"/>
        <CombineArray value="NotColossus"/>
        <Type value="And"/>
    </CValidatorCombine>
    <CActorModel id="FrozenProtossStasisVoid" parent="ModelAnimationStyleContinuous">
        <On Terms="Behavior.IceWaveFrozen.On" Send="Create"/>
        <On Terms="Behavior.IceWaveFrozen.Off" Send="AnimBracketStop BSD"/>
        <Remove Terms="ActorOrphan" Send="Destroy"/>
        <Model value="FrozenProtossVoid"/>
        <Host Subject="_Unit"/>
        <HostSiteOps Ops="SOpAttachCenter"/>
        <AutoScaleFactor value="1.000000"/>
        <Inherits index="BaseModelScale" value="1"/>
        <Inherits index="CloakEffect" value="1"/>
        <Inherits index="Scale" value="1"/>
        <Inherits index="Visibility" value="1"/>
    </CActorModel>
    <CModel id="FrozenProtossVoid" parent="PersistentSpellFX">
        <Model value="Assets\Effects\Protoss\StasisTomb\StasisTomb.m3"/>
        <EditorCategories value="Race:Protoss"/>
        <Events>
            <Anim value="Birth"/>
            <Name value="ArbiterMP_StasisFieldOn"/>
            <Type value="Sound"/>
            <Time value="0.010000"/>
            <Payload value="ArbiterMP_StasisFieldOn"/>
        </Events>
        <Events>
            <Anim value="Death"/>
            <Name value="ArbiterMP_StasisFieldOff"/>
            <Type value="Sound"/>
            <Time value="0.010000"/>
            <Payload value="ArbiterMP_StasisFieldOff"/>
        </Events>
        <ScaleMax value="0.250000,0.250000,0.250000"/>
        <ScaleMin value="0.250000,0.250000,0.250000"/>
    </CModel>
</Catalog>