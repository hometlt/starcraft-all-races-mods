        <On Terms="UnitMovementUpdate.*.Walk; IsStatus HybridSize 3" Send="StatusSet ShadowCreate 3"/>
        <On Terms="ActorCreation" Send="StatusSet ShadowCreate 0"/>
        <On Terms="ActorCreation" Send="StatusSet HybridSize 2"/>
