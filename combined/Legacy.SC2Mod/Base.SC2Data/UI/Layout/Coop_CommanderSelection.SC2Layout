        <Frame type="Image" name="bg">
            <Anchor relative="$parent"/>
            <Alpha val="255"/>
            <Color val="55,55,55"/>
            <Texture val="Assets/Textures/SC2_UI_Glues_AlliedCommanders_UpgradeBackground.dds"/>

            <Animation name="Lock">
                <Event event="Locked" action="Reset" frame="$parent"/>
                <Event event="Locked" action="Play" frame="$parent"/>
                <Event event="Unlocked" action="DirectionReverse" frame="$parent"/>
                <Event event="Unlocked" action="Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.00" value="255" out="Auto"/>
                    <Key type="Curve" time="0.01" value="100" out="Auto"/>
                </Controller>
            </Animation>
        </Frame>

